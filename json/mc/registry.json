  "softwares": [
    {
      "id": "vanilla",
      "name": "Vanilla",
      "description": "Official Minecraft server",
      "provider": "mojang",
      "type": "VANILLA",
      "api": {
        "versions": {
          "url": "https://mcjars.app/api/v2/builds/{{software}}",
          "fields": ["projectVersionId", "versionId"]
        },
        "build": {
          "url": "https://mcjars.app/api/v2/builds/{{software}}/{{version}}",
          "fields": [
            "id",
            "type",
            "projectVersionId",
            "versionId",
            "name",
            "experimental",
            "created",
            "changes",
            "installation"
          ]
        }
      },
      "mapping": {
        "software": "{{software.id}}",
        "version": "versionId",
        "build": "installation"
      }
    },
