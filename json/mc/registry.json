        },
        "build": {
          "url": "https://mcjars.app/api/v2/builds/{{software}}/{{version}}",
          "fields": [
            "id",
            "type",
            "projectVersionId",
            "versionId",
            "name",
            "experimental",
            "created",
            "changes",
            "installation"
          ]
        }
      },
      "mapping": {
        "software": "{{software.id}}",
        "version": "versionId",
        "build": "installation"
      }
    },
    {
      "id": "fabric",
      "name": "Fabric",
      "description": "Lightweight modded Minecraft server",
      "provider": "fabricmc",
      "type": "FABRIC",
      "api": {
        "versions": {
          "url": "https://mcjars.app/api/v2/builds/{{software}}",
          "fields": ["projectVersionId", "versionId"]
        },
        "build": {
          "url": "https://mcjars.app/api/v2/builds/{{software}}/{{version}}",
          "fields": [
            "id",
            "type",
            "projectVersionId",
            "versionId",
